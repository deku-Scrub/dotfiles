[user]
    name = David Rodriguez
    email = david.rodriguez@caci.com
[core]
    editor = vim
[alias]
    l = log --oneline --decorate --graph --all
    s = status
    ss = status -s
    c = commit
    cm = commit -m
    bvv = branch -vv
    b = branch
    bd = branch -d
    bnm = branch --no-merged
    bm = branch --merged
    ch = checkout
    f = fetch --all
    p = push
    m = merge
    a = add
    d = diff
    dc = diff --cached
[credential]
    helper = cache
[init]
	defaultBranch = main
